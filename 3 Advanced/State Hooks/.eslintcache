 useState(false);\n\n\n  // useEffect runs after every components evaluation\n  useEffect(() => {\n    console.log(\">>>>>>.\", ifStoredUserLoggedIn)\n    if (ifStoredUserLoggedIn==='1') {\n      // after setting it function will run again.\n\n      setIsLoggedIn(true);\n    }\n    return () => {};\n  }, []);\n\n  const loginHandler = (email, password) => {\n    // We should of course check email and password\n    // But it's just a dummy/ demo anyways\n    setIsLoggedIn(true);\n    localStorage.setItem(\"isLoggedIn\", \"1\");\n  };\n\n  const logoutHandler = () => {\n    setIsLoggedIn(false);\n\n    localStorage.removeItem('isLoggedIn')\n\n  };\n\n  return (\n    <AuthContext.Provider value={{\n      isLoggedIn:false\n    }}>\n\n      <MainHeader isAuthenticated={isLoggedIn} onLogout={logoutHandler} />\n      <main>\n        {!isLoggedIn && <Login onLogin={loginHandler} />}\n        {isLoggedIn && <Home onLogout={logoutHandler} />}\n      </main>\n      <CountDown/>\n    </AuthContext.Provider>\n  );\n}\n\nexport default App;\n","/home/ashuthosh/Desktop/React 2022/Learn-React-/3 Advanced/State Hooks/src/components/Home/Home.js",[],"/home/ashuthosh/Desktop/React 2022/Learn-React-/3 Advanced/State Hooks/src/components/MainHeader/MainHeader.js",[],"/home/ashuthosh/Desktop/React 2022/Learn-React-/3 Advanced/State Hooks/src/components/Login/Login.js",[],"/home/ashuthosh/Desktop/React 2022/Learn-React-/3 Advanced/State Hooks/src/components/MainHeader/Navigation.js",[],"/home/ashuthosh/Desktop/React 2022/Learn-React-/3 Advanced/State Hooks/src/components/UI/Card/Card.js",[],"/home/ashuthosh/Desktop/React 2022/Learn-React-/3 Advanced/State Hooks/src/components/UI/Button/Button.js",[],"/home/ashuthosh/Desktop/React 2022/Learn-React-/3 Advanced/State Hooks/src/components/store/auth-context.js",[],"/home/ashuthosh/Desktop/React 2022/Learn-React-/3 Advanced/State Hooks/src/components/Home/CountDown.js",["47"],{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},